<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <g:HorizontalPanel>
	  <g:SimplePanel ui:field="mapPanel">
	  </g:SimplePanel>
	  <g:VerticalPanel ui:field="mapLegend">
	    <g:HorizontalPanel>
	  		<g:Image url="../images/stillicon.png" width='20px'></g:Image>
	  		<g:Label>Still</g:Label>
	  	</g:HorizontalPanel>
	  	<g:HorizontalPanel>
	  		<g:Image url="../images/walkicon.png" width='20px'></g:Image>
