	  	</g:HorizontalPanel>
	  	<g:HorizontalPanel>
	  		<g:Image url="../images/runicon.png" width='20px'></g:Image>
	  		<g:Label>Running</g:Label>
	  	</g:HorizontalPanel>
	  	<g:HorizontalPanel>
	  		<g:Image url="../images/bikeicon.png" width='20px'></g:Image>
	  		<g:Label>Biking</g:Label>
	  	</g:HorizontalPanel>
	  	<g:HorizontalPanel>
	  		<g:Image url="../images/driveicon.png" width='20px'></g:Image>
	  		<g:Label>Driving</g:Label>
	  	</g:HorizontalPanel>
	  </g:VerticalPanel>
