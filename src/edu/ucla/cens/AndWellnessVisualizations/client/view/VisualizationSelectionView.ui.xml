<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  
  <ui:style>
  	.iconImage {
  		width: 40px;
  		height: 40px;
  		margin-left: 15px;
  	}
  </ui:style>
  
  <g:HorizontalPanel>
  	<g:Image url="../images/calendaricon.jpeg" ui:field="calendarSelection" addStyleNames="{style.iconImage}"></g:Image>
  	<g:Image url="../images/mapicon.jpeg" ui:field="mapSelection" addStyleNames="{style.iconImage}"></g:Image>
  	<g:Image url="../images/charticon.jpg" ui:field="chartSelection" addStyleNames="{style.iconImage}"></g:Image>
  </g:HorizontalPanel>
</ui:UiBinder>