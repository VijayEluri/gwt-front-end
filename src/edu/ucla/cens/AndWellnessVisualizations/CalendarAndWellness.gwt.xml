<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='calendarandwellness'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.http.HTTP" />
  <inherits name="com.google.gwt.resources.Resources" />
  <inherits name="com.google.gwt.xml.XML"/>
  <inherits name="com.google.gwt.i18n.I18N"/>
  <inherits name="com.google.code.p.gwtchismes.GWTChismes"/>
  <inherits name="com.google.gwt.maps.GoogleMaps"/>

  <!-- GWT logging -->
  <inherits name='com.google.gwt.logging.Logging' />    
  <set-property name='gwt.logging.enabled' value='FALSE' />  

  <!-- Property name to determine whether or not we are debugging -->
  <define-property name="deployStatus" values="RELEASE,DEBUG" />
  <set-property name="deployStatus" value="RELEASE" />
  
  <!-- Use different constant depending on whether or not we are debugging -->
  	<replace-with class='edu.ucla.cens.AndWellnessVisualizations.client.DeployStatus.Release'>
		<when-type-is class='edu.ucla.cens.AndWellnessVisualizations.client.DeployStatus' />
		<when-property-is name='deployStatus' value='RELEASE' />
	</replace-with>
	
	<replace-with class='edu.ucla.cens.AndWellnessVisualizations.client.DeployStatus.Debug'>
		<when-type-is class='edu.ucla.cens.AndWellnessVisualizations.client.DeployStatus' />
		<when-property-is name='deployStatus' value='DEBUG' />
	</replace-with>
  
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='edu.ucla.cens.AndWellnessVisualizations.client.CalendarAppControllerEntryPoint'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>

</module>
